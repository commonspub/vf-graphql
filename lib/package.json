{
  "name": "@valueflows/vf-graphql",
  "version": "0.0.1",
  "description": "Reference GraphQL implementation of the ValueFlows spec",
  "main": "index.js",
  "scripts": {
    "dev": "npm-watch",
    "test": "tape tests/**/*.js | faucet"
  },
  "watch": {
    "test": {
      "patterns": [
        "tests",
        "../schemas"
      ],
      "extensions": "js,gql"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valueflows/vf-graphql.git"
  },
  "keywords": [
    "ValueFlows",
    "REA",
    "GraphQL",
    "grammar"
  ],
  "author": "ValueFlows contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valueflows/vf-graphql/issues"
  },
  "homepage": "https://github.com/valueflows/vf-graphql#readme",
  "dependencies": {
    "@valueflows/vf-graphql-schemas": "0.0.1",
    "graphql": "^14.3.0",
    "graphql-tools": "^4.0.4"
  },
  "devDependencies": {
    "faucet": "0.0.1",
    "npm-watch": "^0.6.0",
    "tape": "^4.10.0",
    "tape-catch": "^1.0.6"
  }
}
